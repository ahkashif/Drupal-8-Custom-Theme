
<div class="card col-md-6" style="height: 200px;">
	<section{{ attributes.addClass(classes) }}>
	  {{ title_prefix }}
	  {% if label %}
	    <h2{{ title_attributes.addClass('block-title') }}>{{ label }}</h2>
	  {% endif %}
	  {{ title_suffix }}

	  {% block content %}
	    {{ content }}
	  {% endblock %}
	</section>

</div>
<br>
